{
  "name": "warden_bw_to_viewer",
  "title": "WARDEN: BigWig viewer",
  "summary": "Generate bookmarks to Protein Paint viewer for BigWig Files",
  "dxapi": "1.0.0",
  "version": "1.0.0",
  "inputSpec": [
    {
      "name": "viewer",
      "class": "file",
      "optional": false,
      "help": ""
    },
    {
      "name": "bigwig_files",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "viewer_bookmark",
      "class": "record",
      "help": "",
      "optional": true
    }
  ],
  "timeoutPolicy": {
    "*": {
      "hours": 1
    }
  },
  "runSpec": {
    "interpreter": "bash",
    "file": "src/warden_bw_to_viewer.sh",
    "distribution": "Ubuntu",
    "release": "16.04",
    "version": "0"
  },
  "regionalOptions": {
    "azure:westus": {
      "systemRequirements": {
        "*": {
          "instanceType": "azure:mem2_ssd1_x1"
        }
      }
    },
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_x2"
        }
      }
    }
  },
  "authorizedUsers": [
    "org-stjude_cloud",
    "org-stjude_cloud_app_runners"
  ]
}