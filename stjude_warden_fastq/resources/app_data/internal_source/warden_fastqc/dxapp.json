{
  "name": "warden_fastqc",
  "title": "WARDEN: FASTQC",
  "summary": "FASTQC Analysis",
  "dxapi": "1.0.0",
  "version": "1.0.1",
  "inputSpec": [
    {
      "name": "fastq_input",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.gz"
      ],
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "html_file",
      "class": "file",
      "patterns": [
        "*.html"
      ],
      "help": ""
    },
    {
      "name": "zip_file",
      "class": "file",
      "patterns": [
        "*.zip"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 12
      }
    },
    "execDepends": [
      {
        "name": "openjdk-8-jre"
      },
      {
        "name": "default-jre-headless"
      }
    ],
    "interpreter": "bash",
    "file": "src/warden_fastqc.sh",
    "distribution": "Ubuntu",
    "release": "16.04",
    "version": "0"
  },
  "access": {
    "network": [
      "*"
    ]
  },
  "regionalOptions": {
    "azure:westus": {
      "systemRequirements": {
        "*": {
          "instanceType": "azure:mem2_ssd1_x4"
        }
      }
    },
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_x8"
        }
      }
    }
  },
  "authorizedUsers": [
    "org-stjude_cloud",
    "org-stjude_cloud_app_runners"
  ]
}