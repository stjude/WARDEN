{
  "name": "warden_combine_counts",
  "title": "WARDEN: Combine HTSeq Counts",
  "summary": "Combine HTSeq Count files into a single file",
  "dxapi": "1.0.0",
  "version": "1.0.4",
  "inputSpec": [
    {
      "name": "count_files",
      "label": "count Files",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "sample_files",
      "label": "sample Lists",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "name_value",
      "label": "Name extension",
      "class": "string",
      "optional": true,
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "count_file",
      "label": "count file",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "sample_file",
      "label": "sample file",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "timeoutPolicy": {
    "*": {
      "hours": 24
    }
  },
  "runSpec": {
    "interpreter": "bash",
    "file": "src/warden_combine_counts.sh",
    "distribution": "Ubuntu",
    "release": "16.04",
    "version": "0"
  },
  "regionalOptions": {
    "azure:westus": {
      "systemRequirements": {
        "*": {
          "instanceType": "azure:mem2_ssd1_x2"
        }
      }
    },
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_x4"
        }
      }
    }
  },
  "access": {
    "project": "CONTRIBUTE",
    "network": [
      "*"
    ]
  },
  "authorizedUsers": [
    "org-stjude_cloud",
    "org-stjude_cloud_app_runners"
  ]
}